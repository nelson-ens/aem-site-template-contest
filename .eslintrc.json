{
    "extends": [
        "plugin:prettier/recommended"
    ],
    "globals": {
        "document":"readonly"
    },
    "rules": {
        "no-unused-vars": ["warn", { "vars": "all", "args": "after-used", "ignoreRestSiblings": false }]
    },
    "parserOptions": {
        "ecmaVersion": 2017
    },
    "env": {
        "es6": true
    }
}